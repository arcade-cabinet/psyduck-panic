{
  "name": "psyduck-panic",
  "version": "1.0.0",
  "description": "Psyduck Panic: Evolution Deluxe - A browser-based game",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcade-cabinet/psyduck-panic.git"
  },
  "keywords": [
    "game",
    "astro",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arcade-cabinet/psyduck-panic/issues"
  },
  "homepage": "https://github.com/arcade-cabinet/psyduck-panic#readme",
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.17.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@biomejs/biome": "~2.3.15",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^28.0.0",
    "playwright": "^1.58.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}

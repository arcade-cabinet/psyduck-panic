{
  "name": "cognitive-dissonance",
  "version": "3.0.0",
  "private": true,
  "main": "index.native.tsx",
  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:web": "expo export -p web",
    "build:android": "cd android && ./gradlew assembleRelease",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "check-imports": "node scripts/check-babylon-imports.js",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e:web": "playwright test",
    "test:e2e:mobile": "maestro test .maestro/",
    "test:e2e:mobile:android": "maestro test .maestro/ --platform android",
    "test:e2e:mobile:ios": "maestro test .maestro/ --platform ios"
  },
  "dependencies": {
    "@babylonjs/core": "^8.51.2",
    "@babylonjs/havok": "^1.3.11",
    "expo": "~55.0.0-preview.11",
    "expo-build-properties": "~55.0.7",
    "expo-camera": "~55.0.6",
    "expo-dev-client": "~55.0.6",
    "expo-haptics": "~55.0.6",
    "expo-speech": "~55.0.6",
    "gsap": "^3.14.2",
    "miniplex": "^2.0.0",
    "miniplex-react": "^2.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.2",
    "react-native-web": "^0.21.2",
    "reactylon": "^3.5.4",
    "seedrandom": "^3.0.5",
    "tone": "^14.9.17",
    "yuka": "^0.7.8",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.6",
    "@biomejs/biome": "2.4.1",
    "@expo/metro-config": "~55.0.6",
    "@playwright/test": "^1.58.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.11",
    "@types/react": "~19.2.14",
    "@types/seedrandom": "^3.0.8",
    "@types/yuka": "^0.7.4",
    "babel-plugin-reactylon": "^1.3.1",
    "babel-preset-expo": "^55.0.5",
    "fast-check": "^4.5.3",
    "jest": "^29.7.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=22.0.0"
